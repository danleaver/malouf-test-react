   setIndicatorPos(tmpPos)\n    setAnimateOver(false)\n   \n    //resets css\n    let tmp = mainDivRef.current.offsetWidth\n   }, [slidePos])\n\n  useEffect(() => {\n    setIndicatorTitle(pics[ wrapPos(indicatorPos)- 1 ].title)\n  }, [indicatorPos])\n\n  const wrapPos = (pos) => {\n    if (pos > pics.length) {\n      return 1\n    } else if (pos < 1) {\n      return pics.length\n    }\n    return pos\n  }\n\n  const nextSlide = (n) => {\n    if (n === -1) {\n      setAnimateInLeft(true)\n    } else { \n      setAnimateInRight(true)\n    }\n\n    setIndicatorPos(slidePos + n)\n\n    setTimeout(() => {\n      setAnimateOver(true)\n      setAnimateInRight(false)\n      setAnimateInLeft(false)\n      setSlidePos(slidePos + n)\n    }, 330)\n  }\n\n  const chooseSlide = (k) => {\n    if (k === slidePos - 1) {\n      nextSlide(-1)\n    } else if (k === slidePos + 1) {\n      nextSlide(1)\n    } else {\n      setIndicatorPos(k)\n      setSlidePos(k)\n    }\n  }\n\n  return (\n    <div\n      css={css`\n        overflow-x: hidden;\n        margin-top: -13%;\n\n        @media (max-width: 1024px) {\n          margin-top: -18%;\n        }\n\n        @media (max-width: 900px) {\n          margin-top: -25%;\n        }\n\n        @media (max-width: 767px) {\n          margin-top: -35%;\n        }\n        \n      `}\n    >\n      <div\n        css={css`\n          position: relative;\n          left: -100%;\n\n          @media (max-width: 1250px) {\n            left: -110%;\n          }\n\n          @media (max-width: 875px) {\n            left: -93%;\n          }\n\n          @media (max-width: 767px) {\n            left: -70%;\n          }\n        `}\n      >\n        <div \n          ref={mainDivRef}\n          css={css`\n            display: flex;\n            width: 300%;\n            justify-content: center;\n\n            @media (max-width: 875px) {\n              width: 260%;\n            }\n\n            @media (max-width: 767px) {\n              width: 200%;\n          